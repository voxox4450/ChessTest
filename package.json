{
  "name": "chess-puzzle-app",
  "version": "1.0.0",
  "description": "Chess puzzle training application",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "frontend": "cd frontend && npm start",
    "backend": "cd backend && node server.js",
